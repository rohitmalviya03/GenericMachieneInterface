package model;

public class EditCaseFlagSession {


	private final static EditCaseFlagSession instance = new EditCaseFlagSession();

	public static EditCaseFlagSession getInstance() {
		return instance;
	}

	private Boolean isEdit = false;
	public long caseId;
	private Boolean isEditFromDashboard=false;



	public Boolean getIsEditFromDashboard() {
		return isEditFromDashboard;
	}
	public void setIsEditFromDashboard(Boolean isEditFromDashboard) {
		this.isEditFromDashboard = isEditFromDashboard;
	}
	public Boolean getIsEdit() {
		return isEdit;
	}
	public void setIsEdit(Boolean isEdit) {
		this.isEdit = isEdit;
	}
	public long getCaseId() {
		return caseId;
	}
	public void setCaseId(long caseId) {
		this.caseId = caseId;
	}






}
