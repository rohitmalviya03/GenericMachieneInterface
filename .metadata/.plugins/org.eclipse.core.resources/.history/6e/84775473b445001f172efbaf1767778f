package model;

public class WorkingHours {
	  private int month = 0;
	    private double hoursNormal = 0;
	    private double hoursTravel = 0;
	    private double hoursOvertime = 0;
	    private double hoursTotalCumulative = 0;



		public WorkingHours(int month, double hoursNormal, double hoursTravel, double hoursOvertime,
				double hoursTotalCumulative) {
			super();
			this.month = month;
			this.hoursNormal = hoursNormal;
			this.hoursTravel = hoursTravel;
			this.hoursOvertime = hoursOvertime;
			this.hoursTotalCumulative = hoursTotalCumulative;
		}
		public int getMonth() {
			return month;
		}
		public void setMonth(int month) {
			this.month = month;
		}
		public double getHoursNormal() {
			return hoursNormal;
		}
		public void setHoursNormal(double hoursNormal) {
			this.hoursNormal = hoursNormal;
		}
		public double getHoursTravel() {
			return hoursTravel;
		}
		public void setHoursTravel(double hoursTravel) {
			this.hoursTravel = hoursTravel;
		}
		public double getHoursOvertime() {
			return hoursOvertime;
		}
		public void setHoursOvertime(double hoursOvertime) {
			this.hoursOvertime = hoursOvertime;
		}
		public double getHoursTotalCumulative() {
			return hoursTotalCumulative;
		}
		public void setHoursTotalCumulative(double hoursTotalCumulative) {
			this.hoursTotalCumulative = hoursTotalCumulative;
		}



}
